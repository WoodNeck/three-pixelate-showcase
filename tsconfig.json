{
	"compilerOptions": {
		"module": "commonjs",
		"target": "es5",
		"sourceMap": true,
		"lib": ["es2015", "dom"],
		"baseUrl": "./",
		"paths": {
			"@/*": ["src/*"]
		},
		"strict": true,
		"noImplicitAny": true,
		"suppressImplicitAnyIndexErrors": true,
		"downlevelIteration": true
	},
	"exclude": [
		"node_modules"
	]
}
